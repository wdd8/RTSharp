<Window x:Class="RTSharp.DataProvider.Qbittorrent.Plugin.Views.MainWindow"
        xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:vm="clr-namespace:RTSharp.DataProvider.Qbittorrent.Plugin.ViewModels"
        xmlns:i="clr-namespace:Projektanker.Icons.Avalonia;assembly=Projektanker.Icons.Avalonia"
        mc:Ignorable="d"
        Width="650" Height="360"
        x:DataType="vm:MainWindowViewModel"
        Title="{Binding Title}">

    <Design.DataContext>
        <vm:MainWindowViewModel />
    </Design.DataContext>

	<Window.Styles>
		<Style Selector="TextBlock,TextBox">
			<Setter Property="VerticalAlignment" Value="Center" />
		</Style>
		<Style Selector="Panel > Grid">
			<Setter Property="VerticalAlignment" Value="Top" />
		</Style>
	</Window.Styles>

	<StackPanel Orientation="Horizontal" VerticalAlignment="Top">
        
    </StackPanel>
</Window>